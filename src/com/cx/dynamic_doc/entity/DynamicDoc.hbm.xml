<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.cx.dynamic_doc.entity.DynamicDoc" table="t_dynamic_document">
        <id name="id" type="java.lang.String">
            <column name="document_id" length="32"/>
            <generator class="uuid.hex" />
        </id>

        <property name="title" type="java.lang.String">
            <column name="title" length="50" />
        </property>

        <property name="time" type="java.lang.String">
            <column name="time" length="36"/>
        </property>

        <property name="count" type="java.lang.Long">
            <column name="count" />
        </property>

        <property name="link" type="java.lang.String">
            <column name="link" length="100"/>
        </property>

        <property name="reserve" type="java.lang.String">
            <column name="reserve" length="100"/>
        </property>

    </class>
</hibernate-mapping>